<div class="container mt-2">
    <h2>User Profile</h2>
    <ul class="list-group list-group-flush">
        {{#each this.userData as |data|}}
            <li class="list-group-item">{{ data }}</li>
        {{/each}}
    </ul>
</div>

<!-- The help modal -->
{{#> layouts/modal-layout type="help" title="User Help" }}
    {{#*inline "form-content"}}
        {{> includes/help/user }}
    {{/inline}}
{{/layouts/modal-layout}}